# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Form Builder Configuration
NEXT_PUBLIC_ENABLE_FORM_BUILDER=true
NEXT_PUBLIC_FORM_BUILDER_MAX_FIELDS=100
NEXT_PUBLIC_FORM_BUILDER_AUTOSAVE_INTERVAL=30000

# Storage Configuration
NEXT_PUBLIC_FORM_ATTACHMENTS_BUCKET=form-attachments
NEXT_PUBLIC_MAX_UPLOAD_SIZE=10485760

# API Configuration
NEXT_PUBLIC_API_BASE_URL=/api
FORM_SUBMISSION_WEBHOOK_URL=
FORM_NOTIFICATION_EMAIL=

# Feature Flags
NEXT_PUBLIC_ENABLE_FORM_TEMPLATES=true
NEXT_PUBLIC_ENABLE_FORM_ANALYTICS=true
NEXT_PUBLIC_ENABLE_FORM_AI_BUILDER=false

# Organization Settings
NEXT_PUBLIC_DEFAULT_ORGANIZATION_ID=